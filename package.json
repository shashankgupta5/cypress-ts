{
  "name": "cypress-ts",
  "version": "1.0.0",
  "description": "Cypress typescript project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shashankgupta5/cypress-ts.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write \"**/*.{ts,js,json}\"",
    "pretest": "rimraf allure-results allure-report logs screenshots",
    "cy:run": "npm run pretest && cypress run --headed --browser=chrome",
    "cy:parallel": "cypress-parallel -s cy:run -t 2 -d cypress/e2e",
    "report": "allure generate allure-results --clean -o allure-report && allure open allure-report"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "allure-cypress": "^3.4.3",
    "cypress": "^15.8.2",
    "cypress-iframe": "^1.0.1",
    "cypress-parallel": "^0.15.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-chai-friendly": "^1.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
